{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "shell",
      "label": "my build",
      "command": "gcc",
      "args": [
        "-g",
        "-Wall",
        "${workspaceFolder}\\main\\main.c",
        "${workspaceFolder}\\str_func.c",
        "${workspaceFolder}\\Vector\\vector_object.c",
        "${workspaceFolder}\\LinkedList\\linked_list_withhead.c",
        "${workspaceFolder}\\HashMap\\hash_map.c",
        "${workspaceFolder}\\test\\hash_map_test.c",
        "${workspaceFolder}\\test\\linked_list_withhead_test.c",
        "${workspaceFolder}\\test\\vector_object_test_1.c",
        "${workspaceFolder}\\test\\vector_object_test_2.c",
        "${workspaceFolder}\\test\\str_func_test.c",
        "-std=c89",
        "-ansi",
        "-o",
        "${workspaceFolder}\\main\\main.exe"
        // "-E",
        // "-MD"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": []
    },
    {
      "type": "shell",
      "label": "my run",
      "command": "${workspaceFolder}\\main\\main.exe",
      "group": {
        "kind": "test",
        "isDefault": true
      }
    }
  ]
}
